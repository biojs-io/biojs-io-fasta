{
  "name": "biojs-io-fasta",
  "description": "Parses FASTA files",
  "version": "0.1.7",
  "homepage": "http://github.com/greenify/biojs-io-fasta",
  "repository": {
    "type": "git",
    "url": "git://github.com/greenify/biojs-io-fasta"
  },
  "author": {
    "name": "greenify",
    "email": "greeenify@gmail.com"
  },
  "dependencies": {
    "biojs-utils-seqtools": "^0.1.6",
    "xhr": "^1.17.0"
  },
  "devDependencies": {
    "browserify": "~4.2.0",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.7.0",
    "mocha": "~1.20.1",
    "nock": "^0.43.0",
    "request": "^2.49.0",
    "uglify-js": "~2.4.15"
  },
  "keywords": [
    "biojs",
    "fasta",
    "sequence"
  ],
  "shields": {
    "build": {
      "img": "https://travis-ci.org/greenify/biojs-io-fasta.svg?branch=master",
      "href": "https://travis-ci.org/greenify/biojs-io-fasta"
    }
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha --ui qunit --reporter spec -C",
    "prepublish": "npm run build-browser; node_modules/coffee-script/bin/coffee --bare --compile --output lib/ src/*.coffee",
    "build-browser": "mkdir -p build; node_modules/browserify/bin/cmd.js -t coffeeify --extension='.coffee'  browser.js > build/biojs_io_fasta.min.js"
